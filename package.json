{
  "name": "portfolio-site",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:storybook": "storybook build",
    "lint": "eslint --config ./eslint.config.js \"{src,public,cypress,scripts,.react-router}/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "react-router dev",
    "start:network": "react-router dev --host 0.0.0.0",
    "start:prod": "react-router-serve ./build/server/index.js",
    "storybook": "storybook dev -p 6006",
    "test": "vitest",
    "test:ci": "echo test",
    "test:e2e": "playwright test",
    "test:e2e:init": "playwright install --with-deps",
    "typecheck": "react-router typegen && tsc --noEmit",
  },
  "dependencies": {
    "@react-router/node": "^7.2.0",
    "@react-router/serve": "^7.2.0",
    "isbot": "^5.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.50.0",
    "@react-router/dev": "^7.2.0",
    "@storybook/react-vite": "^10.2.12",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jsdom": "^26.0.0",
    "playwright": "^1.50.0",
    "prettier": "^3.0.0",
    "sass": "^1.85.0",
    "storybook": "^10.2.12",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.0.0",
    "vite": "^6.0.0",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "^5.0.0",
    "vitest": "^3.0.0",
    "workbox-window": "^7.4.0"
  }
}
